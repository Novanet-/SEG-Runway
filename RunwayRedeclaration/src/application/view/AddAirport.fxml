<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" minHeight="100.0"
	minWidth="300.0" prefHeight="100.0" prefWidth="300.0"
	style="-fx-background-color: #363d43;" stylesheets="@../application.css"
	xmlns="http://javafx.com/javafx/8.0.65" fx:controller="application.view.AddAirportController">
	<children>
		<GridPane layoutX="-153.0" layoutY="-80.0" prefHeight="200.0"
			prefWidth="300.0" stylesheets="@../application.css"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="136.0"
					minWidth="10.0" prefWidth="116.0" />
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="182.0"
					minWidth="10.0" prefWidth="164.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Label text="Airport Name:" textFill="#efefef"
					GridPane.halignment="RIGHT">
					<GridPane.margin>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</GridPane.margin>
				</Label>
				<TextField fx:id="txtAirportName" GridPane.columnIndex="1"
					GridPane.halignment="CENTER" />
				<Button fx:id="btnSubmitAirport" mnemonicParsing="false"
					onAction="#handleBtnSubmitAirport"
					style="-fx-border-radius: 10; -fx-background-radius: 10; -fx-border-width: 0; -fx-background-color: #838383;"
					text="Submit" GridPane.columnIndex="1" GridPane.halignment="CENTER"
					GridPane.rowIndex="1" />
			</children>
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</GridPane>
	</children>
	<opaqueInsets>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</opaqueInsets>
</AnchorPane>
